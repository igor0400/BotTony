{
  "name": "bottony",
  "version": "1.0.0",
  "description": "Bot for cooperative game in Minecraft",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file=.production.env ./dist/src/main.js --trace-deprecation",
    "start:dev": "tsc && node --env-file=.development.env ./dist/src/main.js --trace-deprecation",
    "start:watch-dev": "npx nodemon"
  },
  "keywords": [],
  "author": "igor0400",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "mineflayer": "^4.20.1",
    "mineflayer-armor-manager": "^2.0.1",
    "mineflayer-auto-eat": "^4.0.0",
    "mineflayer-collectblock": "^1.4.1",
    "mineflayer-pathfinder": "^2.4.5",
    "mineflayer-pvp": "^1.3.2",
    "mysql2": "^3.10.0",
    "nodemon": "^3.1.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^9.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.4.5"
  }
}
