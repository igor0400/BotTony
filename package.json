{
  "name": "bottony",
  "version": "1.0.0",
  "description": "Bot for cooperative game in Minecraft",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node --env-file=.production.env ./dist/src/main.js --trace-deprecation",
    "start:dev": "tsc && node --env-file=.development.env ./dist/src/main.js --trace-deprecation",
    "start:watch-dev": "npx nodemon"
  },
  "keywords": [],
  "author": "igor0400",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "mineflayer": "^4.14.0",
    "mineflayer-armor-manager": "^2.0.1",
    "mineflayer-auto-eat": "^3.3.6",
    "mineflayer-collectblock": "^1.4.1",
    "mineflayer-pathfinder": "^2.4.5",
    "mineflayer-pvp": "^1.3.2",
    "mysql2": "^3.6.5",
    "nodemon": "^3.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.3.3"
  }
}
